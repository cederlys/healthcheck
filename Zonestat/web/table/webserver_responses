        <table>
            <tr>
                <th>Response Code</th>
% foreach my $n (@ds) {
                    <th><% $n->domainset->name %><br> <% $n->name %></th>
% }
                </tr>
% for my $k (sort {$data{$b}{$ds[0]->id} <=> $data{$a}{$ds[0]->id}} keys %data) {
                <tr class="<% ++$c%2==0?'even':'odd' %>">
                    <td>
                        <% $k %>
% if ($code{$k}) {
                    <% $code{$k} %>
% }
                    </td>
% foreach my $n (@ds) {
                    <td class="numeric"><% sprintf '%0.2f',100*($data{$k}{$n->id}/$n->webservers({https => $https})->count) %>%</td>
% }

                </tr>
% }
            <tr>
                <th colspan="<% 1+@ds %>"><a href="/csv/webserver_responses?https=<% $https %><% $sidstring %>">CSV file</a></th>
            </tr>
        </table>
        
<%init>
my $ws;

my %code = (
    100 => q{Continue},
    101 => q{Switching Protocols},
    102 => q{Processing},
    200 => q{OK},
    201 => q{Created},
    202 => q{Accepted},
    203 => q{Non-Authoritative Information},
