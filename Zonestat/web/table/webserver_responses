        <table>
            <tr>
                <th>Response Code</th>
% foreach my $n (@ds) {
                    <th><% $n->name %></th>
% }
                </tr>
% for my $k (sort {$data{$b}{$ds[0]->id} <=> $data{$a}{$ds[0]->id}} keys %data) {
                <tr class="<% ++$c%2==0?'even':'odd' %>">
                    <td><% $k %></td>
% foreach my $n (@ds) {
                    <td class="numeric"><% sprintf '%0.2f',100*($data{$k}{$n->id}/$n->webservers({https => $https})->count) %>%</td>
% }

                </tr>
% }
            <tr>
                <th colspan="<% 1+@ds %>"><a href="/csv/webserver_responses?https=<% $https %><% $sidstring %>">CSV file</a></th>
            </tr>
        </table>
        
<%init>
my $ws;

my @ds = map {$zs->present->dbx('Testrun')->find($_)} @sid;
$m->abort(404) unless @ds;

my %data = $zs->present->webservers_by_responsecode($https, @ds);
my $sidstring = '&';
$sidstring .= join('&', map {"sid=$_"} @sid);
my $c;
</%init>
