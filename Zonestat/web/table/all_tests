        <table>
            <tr>
                <th>Domain</th>
                <th>Start</th>
                <th>Duration</th>
                <th>Critical</th>
                <th>Error</th>
                <th>Warning</th>
                <th>Notice</th>
                <th>Info</th>
            </tr>
% for my $i (@data) {
                <tr class="<% ++$c%2==0?'even':'odd' %>">
                    <td>
% if ($i->results->count > 0) {
                        <a href="/tests/<% $i->id %>"><% $i->domain %></a>
% } else {
                        <% $i->domain %>
% }
                    </td>
                    <td><% $i->begin(1) %></td>
                    <td class="numeric"><% $i->end - $i->begin %>s</td>
                    <td class="numeric"><% $i->count_critical %></td>
                    <td class="numeric"><% $i->count_error %></td>
                    <td class="numeric"><% $i->count_warning %></td>
                    <td class="numeric"><% $i->count_notice %></td>
                    <td class="numeric"><% $i->count_info %></td>
                </tr>
% }
        </table>
        
<%init>
my @data;
my $pres = $zs->present;
my $rs;

if (defined($sid)) {
    $rs = $pres->dbx('Testrun')->find($sid)->tests;
} else {
    $rs = $pres->all_dnscheck_tests;
}

if (defined($page)) {
    @data = $rs->search({},{rows => 100, page => $page})->all;
} else {
    @data = $rs->all;
}
my $c;
</%init>
<%args>
$page => undef
$sid => undef
</%args>