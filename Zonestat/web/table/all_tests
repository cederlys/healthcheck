        <table>
            <tr>
                <th>Domain</th>
                <th>Start</th>
                <th>Duration</th>
                <th>Critical</th>
                <th>Error</th>
                <th>Warning</th>
                <th>Notice</th>
                <th>Info</th>
            </tr>
% for my $i (@data) {
                <tr class="<% ++$c%2==0?'even':'odd' %>">
                    <td><% $i->domain %></td>
                    <td><% $i->begin(1) %></td>
                    <td class="numeric"><% $i->end - $i->begin %>s</td>
                    <td class="numeric"><% $i->count_critical %></td>
                    <td class="numeric"><% $i->count_error %></td>
                    <td class="numeric"><% $i->count_warning %></td>
                    <td class="numeric"><% $i->count_notice %></td>
                    <td class="numeric"><% $i->count_info %></td>
                </tr>
% }
        </table>
        
<%init>
my @data;
if (defined($page)) {
    @data = $zs->present->all_dnscheck_tests->search({},{rows => 100, page => $page})->all;
} else {
    @data = $zs->present->all_dnscheck_tests->all;
}
my $c;
</%init>
<%args>
$page => undef
</%args>