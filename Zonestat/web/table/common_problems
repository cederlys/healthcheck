        <table>
            <tr>
                <th>%</th>
                <th>Error</th>
            </tr>
% for my $i (@data) {
                <tr class="<% ++$c%2==0?'even':'odd' %>">
                    <td><% sprintf("%5.2f%%",($i->[1] / $total) * 100) %></td>
                    <td><% $i->[0] %></td>
                </tr>
% }
        </table>
        
<%init>
my @data  = $zs->present->number_of_domains_with_message;
my $total = $zs->present->total_tested_domains;
my $c;
</%init>
