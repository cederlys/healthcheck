        <table>
            <tr>
                <th>Highest severity</th>
% foreach my $s (@ds) {
                <th><% $s->name %></th>
% }
            </tr>
% for my $i (qw[critical error warning notice info clear]) {
                <tr class="<% ++$c%2==0?'even':'odd' %>">
                    <td><% uc $i %></td>
% for my $k (@ds) {
                    <td class="numeric"><% 0+$data{$i}{$k->id} %></td>
% }
                </tr>
% }
            <tr>
                <th colspan="<% 1+@ds %>"><a href="/csv/max_severity<% $sidstring %>">CSV file</a></th>
            </tr>
        </table>
        
<%init>
my @ds;

@ds = map {$zs->present->dbx('Testrun')->find($_)} @sid;
$m->abort(404) unless @ds;

my %data  = $zs->present->tests_with_max_severity(@ds);
my $sidstring = '?';
$sidstring .= join('&', map {"sid=$_"} @sid);

my $c;
</%init>
<%args>
@sid
</%args>