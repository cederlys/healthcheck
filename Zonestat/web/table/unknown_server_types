        <table>
            <tr>
                <th>Server Identification String</th>
            </tr>
% for my $i (@data) {
                <tr class="<% ++$c%2==0?'even':'odd' %>">
                    <td><% $i %></td>
                </tr>
% }
        </table>
        
<%init>
my $ds;
if (defined($sid)) {
    $ds = $zs->present->dbx('Domainset')->find($sid);
    $m->abort(404) unless defined($ds);
}

my @data  = $zs->present->unknown_server_strings($ds);
my $c;
</%init>
<%args>
$sid => undef
</%args>