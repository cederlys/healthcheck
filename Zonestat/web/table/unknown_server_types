        <table>
            <tr>
                <th>Server Identification String</th>
            </tr>
% for my $i (@data) {
                <tr class="<% ++$c%2==0?'even':'odd' %>">
                    <td><% $i %></td>
                </tr>
% }
        </table>
        
<%init>
my $zs    = Zonestat->new('/opt/local/share/dnscheck/site_config.yaml');
my @data  = $zs->present->unknown_server_strings;
my $c;
</%init>
